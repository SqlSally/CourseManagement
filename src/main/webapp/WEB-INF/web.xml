    <servlet-class>ga.rugal.servlet.controller.SimpleServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>b</servlet-name>
    <servlet-class>ga.rugal.servlet.controller.InsertServlet</servlet-class>
  </servlet>
  
    <servlet>
    <servlet-name>c</servlet-name>
    <servlet-class>ga.rugal.servlet.controller.UpdateServlet</servlet-class>
  </servlet>
  
    <servlet>
    <servlet-name>d</servlet-name>
