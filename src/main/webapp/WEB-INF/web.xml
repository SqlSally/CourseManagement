  </servlet>
  
    <servlet>
    <servlet-name>admin</servlet-name>
    <servlet-class>ga.rugal.servlet.controller.AdminServlet</servlet-class>
  </servlet>
  
  
   <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>ga.rugal.servlet.controller.LoginServlet</servlet-class>
  </servlet>
   
  
  <servlet>
    <servlet-name>student_select_course</servlet-name>
    <servlet-class>ga.rugal.servlet.controller.StudentSelectCourseServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>student_view_course</servlet-name>
    <servlet-class>ga.rugal.servlet.controller.StudentViewCourseServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>a</servlet-name>
    <url-pattern>/sally</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>b</servlet-name>
    <url-pattern>/pig</url-pattern>
  </servlet-mapping>
  
    <servlet-mapping>
    <servlet-name>c</servlet-name>
    <url-pattern>/pig1</url-pattern>
  </servlet-mapping>
  
    <servlet-mapping>
    <servlet-name>d</servlet-name>
    <url-pattern>/pig2</url-pattern>
  </servlet-mapping>
  

  <servlet-mapping>
    <servlet-name>student</servlet-name>
    <url-pattern>/student</url-pattern>
  </servlet-mapping>
  
